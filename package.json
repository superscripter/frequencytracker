{
  "name": "frequency-tracker",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=@frequency-tracker/api",
    "web": "npm run dev --workspace=@frequency-tracker/web",
    "api": "npm run dev --workspace=@frequency-tracker/api",
    "db:push": "npm run db:push --workspace=@frequency-tracker/database",
    "db:studio": "npm run db:studio --workspace=@frequency-tracker/database",
    "db:generate": "npm run db:generate --workspace=@frequency-tracker/database",
    "send-email": "npx tsx apps/api/src/scripts/sendEmail.ts"
  },
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "sharp": "^0.34.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "pg": "^8.16.3",
    "pino-pretty": "^13.1.2"
  }
}
